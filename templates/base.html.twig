<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{{ 'base.default_title' | trans }}{% endblock %}</title>
    {{ encore_entry_link_tags('style') }}
    {% block stylesheets %}{% endblock %}
    <link rel="icon" href="{{ asset('images/favicon.ico') }}">
</head>
<body>
{{ include('navbar.html.twig') }}
{% block beforeContainer %}{% endblock %}
<div class="container">
    <div class="row">
        <div class="col-md-10">
            {% block body %}
                <h1>{{ 'base.default_body' | trans }}</h1>
            {% endblock %}
        </div>
        <div class="col-md-2">
            {{ render(controller('App\\Controller\\BoutiqueController::plusVendu')) }}
        </div>
    </div>
</div>
{{ encore_entry_script_tags('app') }}
{% block javascripts %}{% endblock %}
</body>
</html>
